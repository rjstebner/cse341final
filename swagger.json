          "application/json"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "post": {
        "tags": [
          "Items"
        ],
        "description": "Create a new item",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "example": "Jungle Vine Ball"
                },
                "description": {
                  "example": "A springy ball made from jungle vines, perfect for Nargacuga to chase and claw at during play."
                },
                "category": {
                  "example": "Nargacuga"
                },
                "price": {
                  "example": 1000
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    }
  }
}