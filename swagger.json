            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "example": "Nargacuga"
                },
                "description": {
                  "example": "A stealthy predator that moves silently through dense jungles, striking swiftly with its razor-sharp tail."
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
