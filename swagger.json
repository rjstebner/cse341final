          "200": {
            "description": "OK"
          }
        }
      },
      "post": {
        "tags": [
          "CreatureSpe"
        ],
        "description": "Create a special creature",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "example": "Shadow"
                },
                "description": {
                  "example": "Shadow is a sleek and agile Nargacuga who loves sneaking around and pouncing during playtime. Heâ€™s searching for a patient companion to bond with."
                },
                "category": {
                  "example": "Nargacuga"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/items": {
      "get": {
        "tags": [
